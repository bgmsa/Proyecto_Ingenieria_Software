{
  "swagger" : "2.0",
  "info" : {
    "description" : "API Rest del microservicio encargado de la gestion de las matriculas de la Universidad de Villamayor.",
    "version" : "1.0.0",
    "title" : "API Universidad Villamayor Matriculas"
  },
  "host" : "localhost:9200",
  "paths" : {
    "/asignaturasMatriculablesByAlumno/{numero_expediente}" : {
      "get" : {
        "tags" : [ "Asignatura" ],
        "summary" : "Obtener las asignaturas matriculables del alumno.",
        "description" : "Obtener la informacion de las asignaturas en las que un alumno se puede matricular.",
        "parameters" : [ {
          "name" : "numero_expediente",
          "in" : "path",
          "description" : "Numero de expediente del alumno.",
          "required" : true,
          "type" : "string"
        } ],
        "responses" : {
          "200" : {
            "description" : "Operacion realizada con exito",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Asignatura"
              }
            }
          },
          "405" : {
            "description" : "Operacion sin realizar"
          },
          "500" : {
            "description" : "Internal Server Error"
          }
        },
        "x-contentType" : "application/json",
        "x-accepts" : "application/json"
      }
    },
    "/grupos_Disponibles" : {
      "get" : {
        "tags" : [ "Grupo" ],
        "summary" : "Obtener los grupos de las asignaturas que se quiere matricular.",
        "description" : "Obtener los grupos para poder matricularse en las asignaturas",
        "parameters" : [ {
          "in" : "body",
          "name" : "asignaturas",
          "description" : "NIF del alumno que se quiere actualizar",
          "required" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/definitions/Asignatura"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Operacion realizada con exito",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Grupo"
              }
            }
          },
          "405" : {
            "description" : "Operacion sin realizar"
          },
          "500" : {
            "description" : "Internal Server Error"
          }
        },
        "x-contentType" : "application/json",
        "x-accepts" : "application/json"
      }
    },
    "/crear_matricula/{numero_expediente}" : {
      "post" : {
        "tags" : [ "Matriculas" ],
        "summary" : "Creacion de una nueva matricula",
        "description" : "Creacion de una nueva matricula en el expediente del alumno indicado a partir de los grupos seleccionados.",
        "parameters" : [ {
          "name" : "numero_expediente",
          "in" : "path",
          "description" : "Numero de expediente del alumno",
          "required" : true,
          "type" : "integer"
        }, {
          "in" : "body",
          "name" : "grupos",
          "description" : "Grupos que el alumno ha decidido matricularse",
          "required" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/definitions/Grupo"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Operacion realizada con exito"
          },
          "405" : {
            "description" : "Operacion sin realizar"
          },
          "500" : {
            "description" : "Internal Server Error"
          }
        },
        "x-contentType" : "application/json",
        "x-accepts" : "application/json"
      }
    },
    "/ver_expediente/{numero_expediente}" : {
      "get" : {
        "tags" : [ "Matriculas" ],
        "summary" : "Ver las matriculas",
        "description" : "Ver todo el expediente del alumno",
        "parameters" : [ {
          "name" : "numero_expediente",
          "in" : "path",
          "description" : "Numero de expediente del alumno",
          "required" : true,
          "type" : "integer"
        } ],
        "responses" : {
          "200" : {
            "description" : "Operacion realizada con exito",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Matricula"
              }
            }
          },
          "405" : {
            "description" : "Operacion sin realizar"
          },
          "500" : {
            "description" : "Internal Server Error"
          }
        },
        "x-contentType" : "application/json",
        "x-accepts" : "application/json"
      }
    },
    "/realizar_reserva/{numero_expediente}" : {
      "put" : {
        "tags" : [ "Matriculas" ],
        "summary" : "Realizar la reserva",
        "description" : "Realiza la reserva de la ultima matricula dada de alta en el expediente",
        "parameters" : [ {
          "name" : "numero_expediente",
          "in" : "path",
          "description" : "Numero de expediente del alumno",
          "required" : true,
          "type" : "integer"
        } ],
        "responses" : {
          "200" : {
            "description" : "Operacion realizada con exito"
          },
          "405" : {
            "description" : "Operacion sin realizar"
          },
          "500" : {
            "description" : "Internal Server Error"
          }
        },
        "x-contentType" : "application/json",
        "x-accepts" : "application/json"
      }
    }
  },
  "definitions" : {
    "Asignatura" : {
      "type" : "object",
      "required" : [ "Carrera", "Codigo", "Creditos", "Nombre", "Tipo" ],
      "properties" : {
        "Codigo" : {
          "type" : "integer"
        },
        "Nombre" : {
          "type" : "string"
        },
        "Carrera" : {
          "type" : "integer"
        },
        "Tipo" : {
          "type" : "string"
        },
        "Creditos" : {
          "type" : "integer"
        }
      }
    },
    "Grupo" : {
      "type" : "object",
      "required" : [ "Actas", "Anno", "Asignatura", "Codigo", "Miembros", "Profesores", "Tipo" ],
      "properties" : {
        "Codigo" : {
          "type" : "integer"
        },
        "Actas" : {
          "type" : "boolean"
        },
        "Miembros" : {
          "type" : "integer"
        },
        "Tipo" : {
          "type" : "string"
        },
        "Asignatura" : {
          "type" : "integer"
        },
        "Profesores" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "Anno" : {
          "type" : "integer"
        }
      }
    },
    "Asignatura_Matriculada" : {
      "type" : "object",
      "required" : [ "Asignatura", "Nota" ],
      "properties" : {
        "Asignatura" : {
          "$ref" : "#/definitions/Asignatura"
        },
        "Nota" : {
          "type" : "integer"
        }
      }
    },
    "Matricula" : {
      "type" : "object",
      "required" : [ "Anno", "Asignaturas" ],
      "properties" : {
        "Asignaturas" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/Asignatura_Matriculada"
          }
        },
        "Anno" : {
          "type" : "integer"
        }
      }
    }
  }
}